{"ast":null,"code":"import Chart from \"chart.js/auto\";\nexport default {\n  name: \"gradient-line-chart\",\n  props: {\n    title: {\n      type: String,\n      default: \"Sales overview\"\n    },\n    detail1: {\n      type: String,\n      default: \"4% more\"\n    },\n    detail2: {\n      type: String,\n      default: \"in 2021\"\n    }\n  },\n  mounted() {\n    var ctx1 = document.getElementById(\"chart-line\").getContext(\"2d\");\n    var gradientStroke1 = ctx1.createLinearGradient(0, 230, 0, 50);\n    gradientStroke1.addColorStop(1, \"rgba(94, 114, 228, 0.2)\");\n    gradientStroke1.addColorStop(0.2, \"rgba(94, 114, 228, 0.0)\");\n    gradientStroke1.addColorStop(0, \"rgba(94, 114, 228, 0)\");\n    new Chart(ctx1, {\n      type: \"line\",\n      data: {\n        labels: [\"Apr\", \"May\", \"Jun\", \"Jul\", \"Aug\", \"Sep\", \"Oct\", \"Nov\", \"Dec\"],\n        datasets: [{\n          label: \"Mobile apps\",\n          tension: 0.4,\n          borderWidth: 0,\n          pointRadius: 0,\n          borderColor: \"#4BB543 \",\n          backgroundColor: gradientStroke1,\n          // eslint-disable-next-line no-dupe-keys\n          borderWidth: 3,\n          fill: true,\n          data: [50, 40, 300, 220, 500, 250, 400, 230, 500],\n          maxBarThickness: 6\n        }]\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false,\n        plugins: {\n          legend: {\n            display: false\n          }\n        },\n        interaction: {\n          intersect: false,\n          mode: \"index\"\n        },\n        scales: {\n          y: {\n            grid: {\n              drawBorder: false,\n              display: true,\n              drawOnChartArea: true,\n              drawTicks: false,\n              borderDash: [5, 5]\n            },\n            ticks: {\n              display: true,\n              padding: 10,\n              color: \"#fbfbfb\",\n              font: {\n                size: 11,\n                family: \"Open Sans\",\n                style: \"normal\",\n                lineHeight: 2\n              }\n            }\n          },\n          x: {\n            grid: {\n              drawBorder: false,\n              display: false,\n              drawOnChartArea: false,\n              drawTicks: false,\n              borderDash: [5, 5]\n            },\n            ticks: {\n              display: true,\n              color: \"#ccc\",\n              padding: 20,\n              font: {\n                size: 11,\n                family: \"Open Sans\",\n                style: \"normal\",\n                lineHeight: 2\n              }\n            }\n          }\n        }\n      }\n    });\n  }\n};","map":{"version":3,"names":["Chart","name","props","title","type","String","default","detail1","detail2","mounted","ctx1","document","getElementById","getContext","gradientStroke1","createLinearGradient","addColorStop","data","labels","datasets","label","tension","borderWidth","pointRadius","borderColor","backgroundColor","fill","maxBarThickness","options","responsive","maintainAspectRatio","plugins","legend","display","interaction","intersect","mode","scales","y","grid","drawBorder","drawOnChartArea","drawTicks","borderDash","ticks","padding","color","font","size","family","style","lineHeight","x"],"sources":["C:\\Users\\Batuhan Aydemir\\Desktop\\Projects\\gamepanel\\vue-client\\src\\examples\\Charts\\GradientLineChart.vue"],"sourcesContent":["<template>\r\n  <div class=\"card\">\r\n    <div class=\"pb-0 card-header mb-0\">\r\n      <h6>{{ title }}</h6>\r\n      <p class=\"text-sm\">\r\n        <i class=\"fa fa-arrow-up text-success\"></i>\r\n        <span class=\"font-weight-bold\">{{detail1}}</span>\r\n        {{detail2}}\r\n      </p>\r\n    </div>\r\n    <div class=\"p-3 card-body\">\r\n      <div class=\"chart\">\r\n        <canvas id=\"chart-line\" class=\"chart-canvas\" height=\"300\"></canvas>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Chart from \"chart.js/auto\";\r\n\r\nexport default {\r\n  name: \"gradient-line-chart\",\r\n\r\n  props: {\r\n    title: {\r\n      type: String,\r\n      default: \"Sales overview\",\r\n    },\r\n    detail1: {\r\n      type: String,\r\n      default: \"4% more\",\r\n    },\r\n    detail2: {\r\n      type: String,\r\n      default: \"in 2021\",\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    var ctx1 = document.getElementById(\"chart-line\").getContext(\"2d\");\r\n\r\n    var gradientStroke1 = ctx1.createLinearGradient(0, 230, 0, 50);\r\n\r\n    gradientStroke1.addColorStop(1, \"rgba(94, 114, 228, 0.2)\");\r\n    gradientStroke1.addColorStop(0.2, \"rgba(94, 114, 228, 0.0)\");\r\n    gradientStroke1.addColorStop(0, \"rgba(94, 114, 228, 0)\");\r\n    new Chart(ctx1, {\r\n      type: \"line\",\r\n      data: {\r\n        labels: [\"Apr\", \"May\", \"Jun\", \"Jul\", \"Aug\", \"Sep\", \"Oct\", \"Nov\", \"Dec\"],\r\n        datasets: [\r\n          {\r\n            label: \"Mobile apps\",\r\n            tension: 0.4,\r\n            borderWidth: 0,\r\n            pointRadius: 0,\r\n            borderColor: \"#4BB543 \",\r\n            backgroundColor: gradientStroke1,\r\n            // eslint-disable-next-line no-dupe-keys\r\n            borderWidth: 3,\r\n            fill: true,\r\n            data: [50, 40, 300, 220, 500, 250, 400, 230, 500],\r\n            maxBarThickness: 6,\r\n          },\r\n        ],\r\n      },\r\n      options: {\r\n        responsive: true,\r\n        maintainAspectRatio: false,\r\n        plugins: {\r\n          legend: {\r\n            display: false,\r\n          },\r\n        },\r\n        interaction: {\r\n          intersect: false,\r\n          mode: \"index\",\r\n        },\r\n        scales: {\r\n          y: {\r\n            grid: {\r\n              drawBorder: false,\r\n              display: true,\r\n              drawOnChartArea: true,\r\n              drawTicks: false,\r\n              borderDash: [5, 5],\r\n            },\r\n            ticks: {\r\n              display: true,\r\n              padding: 10,\r\n              color: \"#fbfbfb\",\r\n              font: {\r\n                size: 11,\r\n                family: \"Open Sans\",\r\n                style: \"normal\",\r\n                lineHeight: 2,\r\n              },\r\n            },\r\n          },\r\n          x: {\r\n            grid: {\r\n              drawBorder: false,\r\n              display: false,\r\n              drawOnChartArea: false,\r\n              drawTicks: false,\r\n              borderDash: [5, 5],\r\n            },\r\n            ticks: {\r\n              display: true,\r\n              color: \"#ccc\",\r\n              padding: 20,\r\n              font: {\r\n                size: 11,\r\n                family: \"Open Sans\",\r\n                style: \"normal\",\r\n                lineHeight: 2,\r\n              },\r\n            },\r\n          },\r\n        },\r\n      },\r\n    });\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAmBA,OAAOA,KAAI,MAAO,eAAe;AAEjC,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAE3BC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDE,OAAO,EAAE;MACPJ,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EAEDG,OAAOA,CAAA,EAAG;IACR,IAAIC,IAAG,GAAIC,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC;IAEjE,IAAIC,eAAc,GAAIJ,IAAI,CAACK,oBAAoB,CAAC,CAAC,EAAE,GAAG,EAAE,CAAC,EAAE,EAAE,CAAC;IAE9DD,eAAe,CAACE,YAAY,CAAC,CAAC,EAAE,yBAAyB,CAAC;IAC1DF,eAAe,CAACE,YAAY,CAAC,GAAG,EAAE,yBAAyB,CAAC;IAC5DF,eAAe,CAACE,YAAY,CAAC,CAAC,EAAE,uBAAuB,CAAC;IACxD,IAAIhB,KAAK,CAACU,IAAI,EAAE;MACdN,IAAI,EAAE,MAAM;MACZa,IAAI,EAAE;QACJC,MAAM,EAAE,CAAC,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,CAAC;QACvEC,QAAQ,EAAE,CACR;UACEC,KAAK,EAAE,aAAa;UACpBC,OAAO,EAAE,GAAG;UACZC,WAAW,EAAE,CAAC;UACdC,WAAW,EAAE,CAAC;UACdC,WAAW,EAAE,UAAU;UACvBC,eAAe,EAAEX,eAAe;UAChC;UACAQ,WAAW,EAAE,CAAC;UACdI,IAAI